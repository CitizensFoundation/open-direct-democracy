  <p>
    <%= f.label :sequence_number %><br />
    <%= f.text_field :sequence_number %>
  </p>
  <p>
    <%= f.label :stage_sequence_number %><br />
    <%= f.text_field :stage_sequence_number %>
  </p>
  <p>
    <%= f.label :external_date %><br />
    <%= f.text_field :external_date %>
  </p>
  <p>
    <%= f.label :external_id %><br />
